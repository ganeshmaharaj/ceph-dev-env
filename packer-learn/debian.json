{
  "builders": [
    {
      "type": "qemu",
      "vm_name": "test-debian.qcow2",
			"iso_url": "https://cloud.debian.org/images/cloud/buster/20200610-293/debian-10-genericcloud-amd64-20200610-293.qcow2",
      "iso_checksum": "df98b064b1d03cb27156bea8b96eba7a2340910159402a8cb465404516758189",
      "disk_image": true,
      "cpus": 2,
      "memory": 2048,
      "disk_interface": "virtio-scsi",
      "disk_discard": "unmap",
      "disk_compression": true,
      "format": "qcow2",
      "boot_wait": "20s",
      "ssh_username": "builder",
      "ssh_password": "builder",
      "http_directory": "http",
      "qemuargs": [
        ["-serial", "telnet:localhost:43211,server,nowait"],
        ["-smbios", "type=1,serial=ds=nocloud-net;instance-id=centos;seedfrom=http://{{ .HTTPIP }}:{{ .HTTPPort }}/"]
      ],
      "use_default_display": true,
      "headless": true
    }
  ],
	"provisioners": [
    {
      "type": "shell",
      "inline": ["/usr/bin/cloud-init status --wait"]
    },
    {
      "type": "shell",
      "script": "{{template_dir}}/setup.sh"
    }
  ]
}
